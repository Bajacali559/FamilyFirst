    "scoped-http-client": "^0.11.0"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "release-it": "^16.1.5",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0"
  },
  "main": "index.js",
  "scripts": {
    "release": "release-it",
    "test": "mocha test/**/*-test.js --reporter spec",
    "test-with-coverage": "nyc --reporter=text mocha test/**/*-test.js",
