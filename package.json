{
  "name": "hubot-pager-me",
  "description": "PagerDuty integration for Hubot",
  "version": "4.0.0",
  "author": "Josh Nichols <technicalpickles@github.com>",
  "license": "MIT",
  "keywords": [
    "hubot",
    "hubot-scripts",
    "pagerduty"
  ],
  "repository": {
